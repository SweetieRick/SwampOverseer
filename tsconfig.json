{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "outDir": "app",
    "alwaysStrict": true,
    "declaration": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": [
      "es2018"
    ],
    "module": "CommonJS",
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "target": "ES2018",
    "paths": {
      "@client/*": ["client/*"],
      "@template/*": ["template/*"],
      "@functions/*": ["functions/*"],
      "@events/*": ["events/*"],
      "@commands/*": ["commands/*"]
    },
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}
